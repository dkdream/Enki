#include "reader.h"
#include "dump.h"

#include <stdio.h>


int main(int argc, char **argv)
{
    startEnkiLibrary();

    FILE* data = fopen("./test_reader.scm", "r");

    if (!data) {
        printf("unable to open file ./test_reader.scm");
        return -1;
    }

    Reference target;

    while (read(data, &target)) {
        printf("read 0x%p\n", target);
        prettyPrint(stdout, target);
    }

    printf("\n");

    stopEnkiLibrary();
    return 0;
}

